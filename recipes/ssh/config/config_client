SendEnv LANG LC_*

Host *
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null
    LogLevel ERROR

Host github.com
    Hostname github.com
    User git
    IdentityFile ~/.ssh/github_rsa

# proxy to cat-kubectl-mng-prod-aws-usea1-1 through aws-mountaininfra-bastion-us
Host cat-kubectl-mng-prod-aws-usea1-1
    HostName 172.16.9.76
    User juka
    ProxyJump juka@18.208.23.133
    IdentityFile ~/.ssh/nordsec_rsa
    ForwardAgent yes

# proxy to cat-kubectl-mng-stage-aws-usea1-1 through nordsec-bastion-mng-stage-aws-usea1-1
Host cat-kubectl-mng-stage-aws-usea1-1
    HostName 172.17.9.76
    User juka
    ProxyJump juka@52.4.249.219
    IdentityFile ~/.ssh/nordsec_rsa
    ForwardAgent yes

Host cat-kubectl-mng-preprod-aws-usea1-1
    HostName 172.16.17.76
    User juka
    ProxyJump juka@18.208.23.133
    IdentityFile ~/.ssh/nordsec_rsa
    ForwardAgent yes
